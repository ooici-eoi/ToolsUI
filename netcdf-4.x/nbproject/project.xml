<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.java.j2seproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2se-project/3">
            <name>netcdf-4.x</name>
            <source-roots>
                <root id="src.java.dir" name="cdm/src/main/java"/>
                <root id="src.resources.dir" name="cdm/src/main/resources"/>
                <root id="src.java2.dir" name="common\src\java"/>
            </source-roots>
            <test-roots/>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>opendap-2_2</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
        <spring-data xmlns="http://www.netbeans.org/ns/spring-data/1">
            <config-files/>
            <config-file-groups/>
        </spring-data>
    </configuration>
</project>
